			<uxap:ObjectPageSection id="histSections" title="{i18n>changeHist}" titleUppercase="false">
				<uxap:ObjectPageSubSection>
					<table:Table id="idHistoryTable" rows="{edit>/ToChangeHistory/results}" selectionMode="None" visibleRowCount="5" threshold="10">
						<table:extension>
							<m:Toolbar>
								<m:content></m:content>
							</m:Toolbar>
						</table:extension>
						<table:columns>
							<table:Column sortProperty="Indtext" filterProperty="Indtext">
								<table:menu>
									<unified:Menu id="historyContextMenu">
										<unified:items>
											<unified:MenuItem text="Temizle" select="handleHistoryClearFilter" icon="sap-icon://clear-filter"/>
											<unified:MenuItem text="Artan düzeyde sırala" select=".onHistoryOperationSort($event, 'asc')" icon="sap-icon://sort-ascending"/>
											<unified:MenuItem text="Azalan düzeyde sırala" select=".onHistoryOperationSort($event, 'desc')" icon="sap-icon://sort-descending"/>
										</unified:items>
									</unified:Menu>
								</table:menu>
								<m:Label text="{i18n>operation}"/>
								<table:template><m:Text text="{edit>Indtext}"/></table:template>
							</table:Column>
							<table:Column>
								<m:Label text="{i18n>changeLevel}"/>
								<table:template><m:Text text="{edit>Leveltxt}"/></table:template>
							</table:Column>
							<table:Column>
								<m:Label text="{i18n>newValue}"/>
								<table:template><m:Text text="{edit>FNew}"/></table:template>
							</table:Column>
							<table:Column>
								<m:Label text="{i18n>oldValue}"/>
								<table:template><m:Text text="{edit>FOld}"/></table:template>
							</table:Column>
							<table:Column>
								<m:Label text="{i18n>changedBy}"/>
								<table:template><m:Text text="{edit>Username}"/></table:template>
							</table:Column>
							<table:Column hAlign="End">
								<m:Label text="{i18n>changedDate}"/>
								<table:template><m:Text text="{path:'edit>Udate', type: 'sap.ui.model.type.Date', formatOptions: { pattern: 'dd.MM.yyyy' , UTC: true }}"/></table:template>
							</table:Column>
